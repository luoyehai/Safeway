@model Safeway.ViewModel.BasicEntEvaluationBaseVMs.BasicEntEvaluationBaseVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.ProjectId" />
<wt:textbox field="Entity.EnterpriseId" />
<wt:textbox field="Entity.EvluationEnt" />
<wt:datetime field="Entity.EvaluationStartDate" />
<wt:datetime field="Entity.EvaluationEndDate" />
<wt:textbox field="Entity.Evaluator" />
<wt:combobox field="Entity.Status" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>