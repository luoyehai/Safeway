@model Safeway.ViewModel.EnterpriserYearYieldVMs.EnterpriserYearYieldListVM

<wt:searchpanel vm="@Model" reset-btn="true">
    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:datetime field="Searcher.FiscalYear" type="Year" />
        <wt:datetime field="Searcher.Created" />
        <wt:combobox field="Searcher.EnterpriseBasicInfoId" items="Searcher.AllEnterpriseBasicInfos" empty-text="全部" />
    </wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/EnterpriserYearYield/Search"/>
