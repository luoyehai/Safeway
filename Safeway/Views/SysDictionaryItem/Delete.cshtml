@model Safeway.ViewModel.SysDictionaryItemVMs.SysDictionaryItemVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Code" />
<wt:display field="Entity.Name" />
<wt:display field="Entity.Value" />
<wt:display field="Entity.Remark" />
<wt:display field="Entity.Sort" />
<wt:display field="Entity.ChildrenCode" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>