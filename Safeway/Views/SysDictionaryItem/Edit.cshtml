@model Safeway.ViewModel.SysDictionaryItemVMs.SysDictionaryItemVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.Code" />
<wt:textbox field="Entity.Name" />
<wt:textbox field="Entity.Value" />
<wt:textbox field="Entity.Remark" />
<wt:textbox field="Entity.Sort" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>