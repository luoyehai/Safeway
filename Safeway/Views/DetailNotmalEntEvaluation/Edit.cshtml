@model Safeway.ViewModel.DetailNotmalEntEvaluationVMs.DetailNotmalEntEvaluationVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:combobox field="Entity.EvaluateType" />
<wt:combobox field="Entity.EvaluationSelection" />
<wt:textbox field="Entity.DeductionReference" />
<wt:textbox field="Entity.Deduction" />
<wt:textbox field="Entity.DeductionDescription" />
<wt:combobox field="Entity.NormalEntEvaluationId" items="AllNormalEntEvaluations"/>
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>