@model Safeway.ViewModel.EnterpriseReviewElementVMs.EnterpriseReviewElementVM

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:display field="Entity.ElementName" />
        <wt:display field="Entity.Category" />
        <wt:display field="Entity.Level" />
        <wt:display field="Entity.Order" />
        <wt:display field="Entity.TotalScore" />
        <wt:display field="Entity.ID" />
    </wt:row>
    <wt:row align=" AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</wt:form>