@model Safeway.ViewModel.EntRegularCheckElementVMs.EntRegularCheckElementVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.ElementName" />
<wt:display field="Entity.CheckContent" />
<wt:display field="Entity.CheckPoint" />
<wt:display field="Entity.Regulations" />
<wt:display field="Entity.Order" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>