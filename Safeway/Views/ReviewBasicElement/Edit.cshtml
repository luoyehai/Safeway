@model Safeway.ViewModel.ReviewBasicElementVMs.ReviewBasicElementVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:combobox field="Entity.ReviewTempType" />
<wt:textbox field="Entity.ElementName" />
<wt:textbox field="Entity.ElementDesc" />
<wt:textbox field="Entity.Order" />
<wt:textbox field="Entity.TotalScore" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>