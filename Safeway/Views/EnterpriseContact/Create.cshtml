@model Safeway.ViewModel.EnterpriseContactVMs.EnterpriseContactVM

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:textbox field="Entity.Dept" />
<wt:textbox field="Entity.Position" />
<wt:textbox field="Entity.Name" />
<wt:textbox field="Entity.Tele" />
<wt:textbox field="Entity.SchoolName" />
<wt:textbox field="Entity.MobilePhone" />
<wt:textbox field="Entity.Email" />
</wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>