@model Safeway.ViewModel.EnterpriseBasicInfoVMs.EnterpriseBasicInfoListVM

<wt:searchpanel vm="@Model" reset-btn="true">
    <wt:row items-per-row="ItemsPerRowEnum.Three">
        <wt:combobox field="Searcher.EnterpriseBasicId" items="Searcher.AllBasicInfos" empty-text="全部" label-width="100" />
        <wt:combobox field="Searcher.CompanyType" items="CompanyTypeList" label-width="100" />
        <wt:combobox field="Searcher.CompanyScale" items="CompanyScaleList" label-width="100" />
        <wt:textbox field="Searcher.Industry" label-width="100" />
        <wt:textbox field="Searcher.Street" label-width="100" />
        <wt:combobox field="Searcher.TermsofTrade" items="TradeMethodList" label-width="100" />
    </wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/EnterpriseBasicInfo/Search" hidden-checkbox="true"/>
