@model Safeway.ViewModel.EnterpriseBasicInfoVMs.EnterpriseBasicInfoVM

    <wt:form vm="@Model">
        <wt:tab>
            <wt:tabheaders>
                <wt:tabheader title="基本信息" />
                <wt:tabheader title="商务信息" />
                <wt:tabheader title="财务信息" />
                <wt:tabheader title="联系人信息" />
                <wt:tabheader title="年收益信息" />
            </wt:tabheaders>
            <wt:tabcontents>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:textbox field="Entity.ComapanyName" />
                        <wt:combobox field="Entity.Province" items="ProvinceNames" link-field="Entity.City" trigger-url="EnterpriseBasicInfo/LoadCities" />
                        <wt:combobox field="Entity.City"  link-field="Entity.District" trigger-url="EnterpriseBasicInfo/LoadDistricts" />
                        <wt:combobox field="Entity.District" />
                        <wt:textbox field="Entity.Street" />
                        <wt:textbox field="Entity.CompanyType" />
                        <wt:textbox field="Entity.ForeignCountry" />
                        <wt:textbox field="Entity.LegalRepresentative" />
                        <wt:combobox field="Entity.CompanyScale" />
                        <wt:textbox field="Entity.Industry" />
                        <wt:textbox field="Entity.NoofEmployees" />
                        <wt:textbox field="Entity.MainProducts" />
                        <wt:combobox field="Entity.TermsofTrade" />
                    </wt:row>
                </wt:tabcontent>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:combobox field="EnterpriseBusinessinfo.SafetyServiceType" />
                        <wt:textbox field="EnterpriseBusinessinfo.OtherSafetyServiceType" />
                        <wt:textbox field="EnterpriseBusinessinfo.CertificateLevel" />
                        <wt:datetime field="EnterpriseBusinessinfo.ExpireDate" type="Date" />
                        <wt:textbox field="EnterpriseBusinessinfo.OriginalServiceCom" />
                        <wt:textbox field="EnterpriseBusinessinfo.Description" />
                    </wt:row>
                </wt:tabcontent>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:textbox field="EnterpriseFinanceInfo.UnifiedSocialCreditCode" />
                        <wt:textbox field="EnterpriseFinanceInfo.Company_Address" />
                        <wt:textbox field="EnterpriseFinanceInfo.Tele_Number" />
                        <wt:textbox field="EnterpriseFinanceInfo.Bank" />
                        <wt:textbox field="EnterpriseFinanceInfo.Account" />
                        <wt:textbox field="EnterpriseFinanceInfo.CustomerReceiptReceiver" search-url="EnterpriseContact/SerachReceiptReceiver" />
                    </wt:row>
                </wt:tabcontent>
                <wt:tabcontent>
                    <div id="contact">
                        <el-table :data="contactList" style="width: 100%" border>
                            <el-table-column prop="Dept"
                                             label="Dept"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Position"
                                             label="Position"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Name"
                                             label="Name">
                            </el-table-column>
                            <el-table-column prop="MobilePhone"
                                             label="MobilePhone"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Email"
                                             label="Email"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Tele"
                                             label="Tele">
                            </el-table-column>
                        </el-table>
                    </div>
                </wt:tabcontent>
                <wt:tabcontent>
                    <div id="yearYield">
                        <el-table :data="contactList" style="width: 100%" border>
                            <el-table-column prop="Dept"
                                             label="Dept"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Position"
                                             label="Position"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Name"
                                             label="Name">
                            </el-table-column>
                            <el-table-column prop="MobilePhone"
                                             label="MobilePhone"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Email"
                                             label="Email"
                                             width="180">
                            </el-table-column>
                            <el-table-column prop="Tele"
                                             label="Tele">
                            </el-table-column>
                        </el-table>
                    </div>
                </wt:tabcontent>
            </wt:tabcontents>
        </wt:tab>
        <wt:hidden field="Entity.ID" />
        <wt:row align="AlignEnum.Right">
            <wt:submitbutton />
            <wt:closebutton />
        </wt:row>
    </wt:form>
<script>
    var contactVM = new Vue({
        el: '#contact',
        data: {
            contactList: [{
                Dept: '',
                Position: '',
                Name: '',
                MobilePhone: '',
                Email: '',
                Tele: ''
            }],
        },
        methods: {
            loadData: function () {

                this.contactList =@Html.Raw(Json.Serialize(Model.EnterpriseContacts));
            }


        },
        created: function () {
            this.loadData();
        }
    });

    var yearYieldVM = new Vue({
        el: '#yearYield',
        data: {
            contactList: [{
                Dept: '',
                Position: '',
                Name: '',
                MobilePhone: '',
                Email: '',
                Tele: ''
            }],
        },
        methods: {
            loadData: function () {
                this.contactList =@Html.Raw(Json.Serialize(Model.EnterpriseContacts));
            }
        },
        created: function () {
            this.loadData();
        }

    })
</script>