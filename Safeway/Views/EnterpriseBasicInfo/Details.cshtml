@model Safeway.ViewModel.EnterpriseBasicInfoVMs.EnterpriseBasicInfoVM

    <wt:form vm="@Model">
        <wt:tab>
            <wt:tabheaders>
                <wt:tabheader title="基本信息" />
                <wt:tabheader title="商务信息" />
                <wt:tabheader title="财务信息" />
                @*<wt:tabheader title="联系人信息" />
                <wt:tabheader title="年收益信息" />*@
            </wt:tabheaders>
            <wt:tabcontents>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:display field="Entity.ComapanyName" />
                        <wt:display field="Entity.Province" />
                        <wt:display field="Entity.City" />
                        <wt:display field="Entity.District" />
                        <wt:display field="Entity.Street" />
                        <wt:display field="Entity.CompanyType" />
                        <wt:display field="Entity.ForeignCountry" />
                        <wt:display field="Entity.LegalRepresentative" />
                        <wt:display field="Entity.CompanyScale" />
                        <wt:display field="Entity.Industry" />
                        <wt:display field="Entity.NoofEmployees" />
                        <wt:display field="Entity.MainProducts" />
                        <wt:display field="Entity.TermsofTrade" />
                    </wt:row>
                </wt:tabcontent>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:display field="EnterpriseBusinessinfo.SafetyServiceType" />
                        <wt:display field="EnterpriseBusinessinfo.OtherSafetyServiceType" />
                        <wt:display field="EnterpriseBusinessinfo.CertificateLevel" />
                        <wt:display field="EnterpriseBusinessinfo.ExpireDate" />
                        <wt:display field="EnterpriseBusinessinfo.OriginalServiceCom" />
                        <wt:display field="EnterpriseBusinessinfo.Description" />
                    </wt:row>
                </wt:tabcontent>
                <wt:tabcontent>
                    <wt:row items-per-row="ItemsPerRowEnum.Two">
                        <wt:display field="EnterpriseFinanceInfo.UnifiedSocialCreditCode" />
                        <wt:display field="EnterpriseFinanceInfo.Company_Address" />
                        <wt:display field="EnterpriseFinanceInfo.Tele_Number" />
                        <wt:display field="EnterpriseFinanceInfo.Bank" />
                        <wt:display field="EnterpriseFinanceInfo.Account" />
                        <wt:display field="EnterpriseFinanceInfo.CustomerReceiptReceiver"  />                       
                    </wt:row>
                </wt:tabcontent>
                @*<wt:tabcontent>
                    <wt:grid vm="EnterpriseContactListVM" url="/EnterpriseContact/Search" />
                </wt:tabcontent>
                <wt:tabcontent>
                    <wt:grid vm="EnterpriserYearYieldListVM" url="/EnterpriserYearYield/Search" />
                </wt:tabcontent>*@
            </wt:tabcontents>
            </wt:tab>
        <wt:row align=" AlignEnum.Right">
            <wt:closebutton />
        </wt:row>
    </wt:form>