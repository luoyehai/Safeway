@model Safeway.ViewModel.ProjectBasicInfoVMs.ProjectBasicInfoVM

<wt:form vm="@Model">
	<wt:quote>确定要删除这条数据么：</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.ProjectName" />
<wt:display field="Entity.ProjectDescription" />
<wt:display field="Entity.ProjectType" />
<wt:display field="Entity.ProjectOnwer" />
<wt:display field="Entity.ProjectMember" />
<wt:display field="Entity.ProjectStartDate" />
<wt:display field="Entity.ProjectEndDate" />
<wt:display field="Entity.ProjectStatus" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align=" AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="删除"/>
        <wt:closebutton />
    </wt:row>
</wt:form>